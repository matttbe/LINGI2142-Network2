config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option ip6addr '::1/128'

config interface 'eth'
	option ifname 'eth0 eth1 eth2 eth3'
	option type 'bridge'
	option stp '1'
	option proto 'static'
	option ip6addr '2001:bb8:1234:1300::6/48'
	option ip6prefix '2001:bb8:1234::/48'

config interface 'lan'
	option ifname 'eth4'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'

